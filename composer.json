{
  "name": "panvagenas/wp-plugin-core-example",
  "version": "0.0.1-dev",
  "description": "WP Plugins Core for developing plugins",
  "license": "GPL-3.0+",
  "keywords": [],
  "authors": [
    {
      "name": "Panagiotis Vagenas",
      "email": "pan.vagenas@gmail.com",
      "homepage": "https://gr.linkedin.com/in/panvagenas",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "panvagenas/wp-plugin-core": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "symfony/var-dumper": "2.7.*",
    "devster/ubench": "1.*"
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "PanWPCoreExample\\": "src/"
    },
    "files": []
  },
  "archive": {
    "exclude": ["/docs", "/tests"]
  }
}
